{
  "name": "afrittella/laravel-pages",
  "description": "Simple markdown pages management for Laravel 5",
  "require": {
    "php": ">=7.1",
    "kalnoy/nestedset": "^4.2",
    "illuminate/container": "~5.4.0",
    "illuminate/contracts": "~5.4.0",
    "illuminate/database": "~5.4.0",
    "graham-campbell/markdown": "^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Andrea Frittella",
      "email": "me.liveset@gmail.com",
      "role": "Chief Architect & Developer",
      "homepage": "http://www.andreafrittella.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Afrittella\\LaravelPages\\": "src/app"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Afrittella\\LaravelPages\\PagesProvider"
      ]
    }
  }
}
